{
	"compilerOptions": {
		"target": "ESNext",
		"moduleDetection": "force",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules"
	]
}
